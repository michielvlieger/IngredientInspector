{
  "expo": {
    "name": "IngredientInspector",
    "slug": "ingredient-inspector",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/hamburger-image.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontend",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan pictures for products."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.frontend"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@morrowdigital/watermelondb-expo-plugin",
        {
          "disableJSI": true
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.6.10",
            "packagingOptions": {
              "pickFirst": [
                "**/libc++_shared.so"
              ]
            }
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow IngredientInspector to access your camera"
        }
      ],
      [
        "@config-plugins/detox",
        {
          "subdomains": "*"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f358d00a-8142-4d6e-8cc4-10d56b1444c8"
      }
    }
  }
}
