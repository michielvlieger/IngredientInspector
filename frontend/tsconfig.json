{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "strict": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@interfaces": [
        "app/interfaces/index.interface.ts"
      ],
      "@services": [
        "app/services/index.interface.ts"
      ],
      "@constants": [
        "app/constants/index.constants.ts"
      ],
      "@models": [
        "app/database/models/index.model.ts"
      ],
      "@schemas": [
        "app/database/schemas/index.schemas.ts"
      ],
      "@seeds": [
        "app/database/seeds/index.seeds.ts"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "outDir": "./dist"
}